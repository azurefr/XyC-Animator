-- Scripts:

local function UUHYN_fake_script() -- botao.abrir 
	local script = Instance.new('LocalScript', botao)

	local botao = script.Parent
	local frame = script.Parent.Parent.topbar
	
	botao.MouseButton1Click:Connect(function()
		-- Alterna a visibilidade do frame
		frame.Visible = not frame.Visible
	end)
	
end
coroutine.wrap(UUHYN_fake_script)()
local function AELAP_fake_script() -- botao.rodar 
	local script = Instance.new('LocalScript', botao)

	local image = script.Parent 
	
	local speedRodar = 0.9 -- valor personalizado 
	
	while true do 
		image.Rotation = image.Rotation + speedRodar -- Aumenta a rotação da imagem
		wait(0.09) -- Aguarda um pequeno intervalo antes de rotacionar novamente
	end
	
end
coroutine.wrap(AELAP_fake_script)()
local function JHGX_fake_script() -- ImageLabel.rodar 
	local script = Instance.new('LocalScript', ImageLabel)

	local image = script.Parent 
	
	local speedRodar = 1 -- valor personalizado 
	
	while true do 
		image.Rotation = image.Rotation + speedRodar -- Aumenta a rotação da imagem
		wait(0.03) -- Aguarda um pequeno intervalo antes de rotacionar novamente
	end
	
end
coroutine.wrap(JHGX_fake_script)()
local function DDFE_fake_script() -- Anim.LocalScript 
	local script = Instance.new('LocalScript', Anim)

	local button = script.Parent  -- Supondo que o botão esteja no mesmo script
	local animationId = "rbxassetid://76361383590591"  -- Substitua com o ID da animação desejada
	local isOn = false  -- Variável para controlar o estado do botão (ON/OFF)
	local currentAnimation = nil  -- Variável para armazenar a animação atual
	
	button.MouseButton1Click:Connect(function()
		isOn = not isOn  -- Alterna o estado ON/OFF
	
		local character = game.Players.LocalPlayer.Character
		if character then
			local humanoid = character:FindFirstChildOfClass("Humanoid")
			if humanoid then
				if isOn then
					-- Mudar a cor do botão para verde quando ON
					button.BackgroundColor3 = Color3.fromRGB(0, 193, 0)
	
					-- Tocar a animação
					local animation = Instance.new("Animation")
					animation.AnimationId = animationId
					currentAnimation = humanoid:LoadAnimation(animation)
					currentAnimation:Play()
				else
					-- Mudar a cor do botão para branco quando OFF
					button.BackgroundColor3 = Color3.fromRGB(154, 154, 154)
	
					-- Parar a animação
					if currentAnimation then
						currentAnimation:Stop()
					end
				end
			end
		end
	end)
	
end
coroutine.wrap(DDFE_fake_script)()
