local function QTVEH_fake_script() -- Anim_3.LocalScript 
	local script = Instance.new('LocalScript', Anim_3)

	local button = script.Parent  -- Supondo que o botão esteja no mesmo script
	local animationId = "rbxassetid://72755165836017"  -- Substitua com o ID da animação desejada
	local isOn = false  -- Variável para controlar o estado do botão (ON/OFF)
	local currentAnimation = nil  -- Variável para armazenar a animação atual
	
	button.MouseButton1Click:Connect(function()
		isOn = not isOn  -- Alterna o estado ON/OFF
	
		local character = game.Players.LocalPlayer.Character
		if character then
			local humanoid = character:FindFirstChildOfClass("Humanoid")
			if humanoid then
				if isOn then
					-- Mudar a cor do botão para verde quando ON
					button.BackgroundColor3 = Color3.fromRGB(0, 193, 0)
	
					-- Tocar a animação em loop
					local animation = Instance.new("Animation")
					animation.AnimationId = animationId
					currentAnimation = humanoid:LoadAnimation(animation)
					currentAnimation:Play()  -- Começa a animação
	
					-- Garantir que a animação continue em loop
					currentAnimation.Looped = true  -- Define para rodar em loop
	
				else
					-- Mudar a cor do botão para branco quando OFF
					button.BackgroundColor3 = Color3.fromRGB(154, 154, 154)
	
					-- Parar a animação
					if currentAnimation then
						currentAnimation:Stop()
					end
				end
			end
		end
	end)
	
end
coroutine.wrap(QTVEH_fake_script)()
